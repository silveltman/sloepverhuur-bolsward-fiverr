---
layout: null
permalink: /products2.json
sloepen:
  - petter
---
{% assign sloep = collections.sloepen[1].data %}
[
  {% for date in huren.dateRange %}
    {
      "id": "{{ date }}",
      "price": 0,
      "url": "/products.json",
      "name": "{{ sloep.title }}",
      "description": "{{ sloep.description }}",
      "image": "{{ sloep.gallery[0].image }}",
      "variants": [
        {
          "variation": [
            {
              "name": "Ochtend",
              "option": "true"
            },
            {
              "name": "Size",
              "option": "Small"
            }
          ],
          "stock": 10,
          "allowOutOfStockPurchases":  true
        }
      ],
      "customFields": [
        {
          "name": "Ochtend",
          "options": "true[{{ sloep.prijzen.ochtend }}]|false"
        },
        {
          "name": "Middag",
          "options": "true[{{ sloep.prijzen.middag }}]|false"
        },
        {
          "name": "Avond",
          "options": "true[{{ sloep.prijzen.avond }}]|false"
        }
      ]
    }{% if forloop.last == false %},{% endif %}
  {% endfor %}
]


